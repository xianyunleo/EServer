    {
        "Name": "Nginx",
        "DirName": "nginx",
        "ServerProcessPath": "nginx.exe",
        "ServerConfPath": "conf/nginx.conf",
        "StartServerCommand": "${ServerProcessPath} -p ${WorkPath} -g 'daemon off;'",
        "ExtraFiles": [
            {
                "Name": "访问日志",
                "Path": "logs/access.log"
            },
            {
